{
  "recurseDepth": 10,
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
    "include": ["src"],
    "exclude":["node_modules"],
    "includePattern": "\\.(jsx|js|ts|tsx)$",
    "excludePattern": "(^|\\/|\\\\)_"
},
"plugins": [
    "node_modules/better-docs/typescript"

],
"typescript": {
    "moduleRoot": "test"
},
 "templates": {
  "better-docs": {
      "name": "Budget Planning"
    }
},
"opts": {
    "template": "node_modules/better-docs",
    "destination": "docs",
    "recurse": true,
    "readme": "README.md"
}
}